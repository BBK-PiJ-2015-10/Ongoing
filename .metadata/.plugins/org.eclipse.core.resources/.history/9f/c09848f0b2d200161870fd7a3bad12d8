		StairCase(1);
		System.out.println("Please work again again please why oase ale ale");

	}

	static void StairCase(int n){
		
		System.out.println("#");
		
	}
	
	private void helper(String input,int target,int count){
		if (count<target){
			input+="#";
			System.out.println(input);
			helper(input,target,count+1);
		}
	}
