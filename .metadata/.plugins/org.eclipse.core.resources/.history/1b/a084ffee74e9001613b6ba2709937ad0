package theappbusiness;

public class JavaSort {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	
	class Student {
		
		private int id;
		
		private String name;
		
		private double cgpa;
		
		public Student(int id, String name, double cgpa){
			this.id=id;
			this.name=name;
			this.cgpa=cgpa;
		}
		
		public int getId() {
		      return id;
		}
		
	    public String getName() {
		      return name;
		}
	    
	    public double getCgpa() {
		      return cgpa;
		}
	    
		}
		
		
	}

}
