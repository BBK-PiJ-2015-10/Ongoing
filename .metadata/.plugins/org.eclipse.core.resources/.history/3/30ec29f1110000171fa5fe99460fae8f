package delivery;

import java.util.Comparator;
import java.util.Map;
import java.util.HashMap;

public class MyComparator implements Comparator<Node<String>> {

	private Map<Node<String>,Number> pathWeights =new HashMap();
	
	public MyComparator(Map<Node<String>, Number> pathWeights) {
		super();
		this.pathWeights = pathWeights;
	}
	

	@Override
	public int compare(Node<String> arg0, Node<String> arg1) {
		Integer arg0val = pathWeights.get(arg0).intValue();
		Integer arg1val = pathWeights.get(arg1).intValue();
		return arg0val.compareTo(arg1val);
	}
	
	
	public int getTotalElements(){
		pathWeights.size();
	}
	
	



	


}
