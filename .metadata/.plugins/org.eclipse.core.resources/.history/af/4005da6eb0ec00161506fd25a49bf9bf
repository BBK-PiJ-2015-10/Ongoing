package theappbusiness;

public class Java1DArray {

	public static void main(String[] args) {
		
		int[] test1 = {0,0,0,0,0};
		
		Java1DArray ja = new Java1DArray();
		ja.runTest(true,test1,1);

	}
	
	
	public boolean evalArray(int[] values){
		
		return false;
	}
	
	public void runTest(boolean expected,int[] values, int testRef){
		String result;
		if (expected==evalArray(values)){
			result="passed";
		}
		else {
			result="failed";
		}
		System.out.println("Test # "+testRef +" " +result);
	}

}
