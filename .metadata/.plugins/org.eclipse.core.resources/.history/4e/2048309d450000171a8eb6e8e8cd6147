package delivery;

public class Person<T> {
	
	private T name;
	
	public T getName() {
		return name;
	}

	public void setName(T name) {
		this.name = name;
	}

	public Person(T name) {
		this.name = name;
	}
	
	@Override
	public int hashCode(){
		return this.name.hashCode();
	}
	
	@Override
	public boolean equals(Object input){
		if (input==this){
			return true;
		}
		if (!(input instanceof Person)){
			return false;
		}
		Person otherNode = ( (Person) input);
		return otherNode.name.equals(this.name);
	}
	

}
