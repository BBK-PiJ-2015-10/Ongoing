package adt.review

object ADTPrac {
  
  def main(args: Array[String] ) : Unit = {
    
    val plus2 = createAdder(a=>a+1)
    println(plus2(50))
    
    val func = twice(a => a * 2 +1)  
    println(func(10))
    
  }
  
  
  def createAdder(num: (Int) => Int) = {
    (input: Int) => input + num(input)
  }
  
  def twice(f1: (Int) => Int) = {
    (input: Int) => f1(f1(input))
  }
  
  
  
  
}