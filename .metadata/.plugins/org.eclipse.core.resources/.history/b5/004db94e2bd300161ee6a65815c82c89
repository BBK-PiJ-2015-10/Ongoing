		}
		
		
		return -1;
	}
	
	public int helper(int target,int pos,int[]A){
		if (A.length<1){
			return -1;
		}
		int mid=pos/2;
		if (A[mid]==target){
			return mid;
		}
		else if (A[mid]<target && mid>0){
			
		}
		else {
			
		}
	}
	
	
	
