package delivery;

public class Hard<T extends Number> {

	private T hardUnit;
	
	public Hard(T hardUnit){
		this.hardUnit=hardUnit;
	}

	public T getHardUnit() {
		return hardUnit;
	}

	public void setHardUnit(T hardUnit) {
		this.hardUnit = hardUnit;
	}
	
	@Override
	public int hashCode(){
		return hardUnit.hashCode();
	}
	
	@Override
	public boolean equals(Object input){
		if (input.equals(this)){
			return true;
		}
		if (!(input instanceof Node)){
			return false;
		}
		((Hard)input);
		//T otherHard = ((Hard) input).getHardUnit();
		//return otherName.equals(this.nodeName);
	}
	
	
}
