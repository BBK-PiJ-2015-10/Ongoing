package multithreading

import actors.Actor

object ScalaBasicActors {
  
  def main(args: Array[String]): Unit = {
    
    Actor.actor(for (i <- 30 to 40) println(fib(i)))  
    
    println("Ale Tonto")
    
    
        
    
  }
  
  def fib(n: Int): Int = {
    if (n<2) 1 
    else fib(n-1)+fib(n-2)}
  
  
}

