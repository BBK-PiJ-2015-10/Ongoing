package delivery;

import java.util.PriorityQueue;

import java.util.Set;
import java.util.HashSet;

public class ScriptTestPerson {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		ScriptTestPerson tr = new ScriptTestPerson();
		tr.run();

	}
	
	
	private PriorityQueue<EncapPerson> personq = new PriorityQueue();
	
	private Set<Person> people = new HashSet();
	
	private Set<Person> other = new HashSet();
	
	
	public void run(){
		
		Person one = new Person("Ale");
		Person two = new Person("Tontolone");
		Person three = new Person("Mamamia");
		Person four = new Person("Biruta");
		
		people.add(one);
		people.add(two);
		people.add(three);
		people.add(four);
			
		people.forEach((n)->personq.add(new EncapPerson(n,1000)));
		
		System.out.println(personq.size());
		
		EncapPerson src = new EncapPerson(one);
		src.setPriority(100);
		personq.remove(src);
		personq.add(src);
		
		
		System.out.println(personq.size());
		
		
		
		
		
		
		
	
		
		
		//one.setPriority
		
		
	}

}
