package theappbusiness;

import java.util.Scanner;
import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileReader;
import java.util.ArrayList;


public class JavaArray {
	
	public static void main(String[] args) {
		
		JavaArray ja = new JavaArray();
		ja.executor();		
		
	}
	
	private Scanner sc;
	
	private String validAddress = "src/theappbusiness/inputJavaArray.txt";
			
	private File file = new File(validAddress);
	
	public void intScanner(String fileName){
		try {
			sc  = new Scanner(new FileReader(fileName));
		} catch (FileNotFoundException ex){
			System.out.println(ex.getMessage());
		}
	}
	
	public void intScanner(){
		try {
			sc  = new Scanner(new FileReader(file));
		} catch (FileNotFoundException ex){
			System.out.println(ex.getMessage());
		}
	}
	
	ArrayList<ArrayList<Integer>> list = new ArrayList();
	
	
	public void executor(){
		
		System.out.println("Welcome to executor");
		
		intScanner();
			int arrayNumbers=sc.nextInt();
			for (int i=1;i<=arrayNumbers;i++){
				int arraySize=sc.nextInt();
					ArrayList<Integer> temp = new ArrayList(); 
					System.out.println("Array "+i +" is size "+arraySize +" with elemenets ");
				for (int k=0;k<arraySize;k++){
					int element=sc.nextInt();
					temp.add(element);
					System.out.print(" " +element);
				}
				list.add(temp);
				System.out.println();
			}
			int queries=sc.nextInt();
			for (int i=0;i<queries;i++){
				int index1=sc.nextInt();
				int index2=sc.nextInt();
				System.out.println(getValue(index1,index2));
			}
	
	}
	
	public String getValue(int firstIndex, int secondIndex){
		if (list.size()>=firstIndex){
			//System.out.println("This has passed " +firstIndex);
			if(list.get(firstIndex-1).size()>=secondIndex){
				//System.out.println("size is " +list.get(firstIndex-1).size());
				return list.get(firstIndex-1).get(secondIndex-1).toString();
			}
		}
		return "ERROR!";
		
	}
	
	

}
