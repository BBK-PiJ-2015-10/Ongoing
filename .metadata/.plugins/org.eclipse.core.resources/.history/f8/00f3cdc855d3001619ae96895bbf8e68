package recursion;

public class Factorial {

	private int getFactorialNTR(int n){
		if (n==1){
			return 1;
		}
		else {
			return n*getFactorialNTR(n-1);
		}	
	}
	
	
	public static void main(String[] args) {
		
		Factorial fact = new Factorial();
		System.out.println(fact.getFactorialNTR(5));
		
		
		
	}
	
}


